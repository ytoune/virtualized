{
  "name": "@ytoune/virtualized",
  "version": "3.1.0",
  "description": "utility for efficiently rendering large lists and tabular data",
  "author": {
    "name": "ytoune"
  },
  "engines": {
    "node": ">= 16"
  },
  "volta": {
    "node": "20.11.0",
    "yarn": "1.22.21"
  },
  "source": "src/index.ts",
  "unpkg": "./dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ytoune/virtualized.git"
  },
  "homepage": "https://github.com/ytoune/virtualized",
  "license": "MIT",
  "scripts": {
    "build": "unbuild && microbundle -f umd --generateTypes=false --no-sourcemap",
    "test": "npm run lint && tsc --noEmit && vitest run --passWithNoTests",
    "lint": "eslint \"{src,scripts}/**/*.{ts,tsx}\"",
    "lint-fix": "eslint \"{src,scripts}/**/*.{ts,tsx}\" --fix",
    "ts-node": "node -r esbuild-register"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/compat": "1.1.1",
    "@eslint/eslintrc": "3.1.0",
    "@eslint/js": "9.11.1",
    "@types/eslint__eslintrc": "2.1.2",
    "@types/eslint__js": "8.42.3",
    "@types/node": "20.11.24",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "esbuild": "0.24.0",
    "esbuild-register": "3.6.0",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-github": "5.0.2",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-prettier": "5.2.1",
    "globals": "15.9.0",
    "microbundle": "0.15.1",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "typescript": "5.6.2",
    "unbuild": "2.0.0",
    "vite": "5.4.7",
    "vitest": "2.1.1"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "esbuild": "0.24.0",
    "vite": "5.4.7"
  },
  "prettier": {
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore"
  }
}
